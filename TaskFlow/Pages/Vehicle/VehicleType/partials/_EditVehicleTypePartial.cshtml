<div class="offcanvas offcanvas-end" tabindex="-1" id="editVehicleTypeOffcanvas" aria-labelledby="editVehicleTypeOffcanvasLabel">
    <div class="offcanvas-header border-bottom">
        <h5 class="offcanvas-title" id="editVehicleTypeOffcanvasLabel">Editar Tipo de Vehículo</h5>
        <button type="button" class="btn-close text-reset" data-bs-dismiss="offcanvas" aria-label="Close"></button>
    </div>
    <div class="offcanvas-body mx-0 flex-grow-0">
        <form method="post" asp-page-handler="EditOrUpdate" asp-route-id="@Model.VehicleTypeUpdate.vehicle_type_id" id="editVehicleTypeForm">
            

            <div class="mb-6">
                <label for="EditVehicleType_Id" class="form-label">Tipo vehículo Id</label>
                <input type="text" id="EditVehicleType_Id" name="VehicleType.vehicle_type_id" class="form-control" placeholder="" disabled/>
            </div>
            <div class="mb-6">
                <label for="EditVehicletype_Name" class="form-label">Nombre</label>
                <input type="text" id="EditVehicletype_Name" name="VehicleType.name" class="form-control" placeholder="" />
            </div>          
            

            <div class="d-flex justify-content-end">
                <button type="submit" class="btn btn-primary me-2">Actualizar</button>
                <button type="reset" class="btn btn-label-secondary" data-bs-dismiss="offcanvas">Cancel</button>
            </div>
        </form>
    </div>
</div>
